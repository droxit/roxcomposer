{
	"index_patterns": ["trace-log"],
	"settings": { "number_of_shards": 1 },
	"mappings": {
		"doc": {
			"properties": {
				"service_name": { "type": "keyword" },
				"event": { "type": "keyword" },
				"message_id": { "type": "keyword" },
				"status": { "type": "keyword" },
				"destination": { "type": "keyword" },
				"type": { "type": "keyword" },
				"time": {
					"type": "date",
					"format": "epoch_millis"
				},
				"start_time": {
					"type": "date",
					"format": "epoch_millis"
				},
				"last_update": {
					"type": "date",
					"format": "epoch_millis"
				},
				"processing_time": {
					"type": "scaled_float",
					"scaling_factor": 100
				}
			}
		}
	}
}
